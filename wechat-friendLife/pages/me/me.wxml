<!--index.wxml-->
<view class="container">
  <view class="userinfo" bindtap="bindViewTap">
    <open-data type="userAvatarUrl" class="userinfo-avatar"></open-data>
    <open-data type='userNickName'></open-data>
        </view>
        <!-- 点赞 -->
        <view class="userinfo">
      <van-rate value="{{ value }}" icon="like"
  void-icon="like-o" bind:change="onChange" />
    </view>
    <!-- 日历 -->
    <van-calendar
  title="日历"
  poppable="{{ false }}"
  show-confirm="{{ false }}"
  class="calendar"
/>
</view>